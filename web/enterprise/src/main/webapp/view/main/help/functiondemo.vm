$(function(){
	
	$(info).each(function(i,item) {
		
		var ulhtml='<li><a href="javascript:void(0)"><i></i>'+item.t+'</a><dl class="subnav"></dl></li>'
		$(".ul3").append(ulhtml);
		
		$(item.d).each(function(i2,item2){
			var ddhtml='<dd><a href="javascript:void(0)" data-id1="'+i+'" data-id2="'+i2+'"><span>'+item2.t+'</span><b class="bg"></b></a></dd>'
			$(".helpleftnav_cont .subnav").eq(i).append(ddhtml);
		})
		
    });
	//$(".ul3>li").eq($(".ul3>li").length-1).addClass("nob");
	$(".ul3>li>a").live("click",function(){
		$(this).addClass("cur").parent().siblings().find("a").removeClass("cur");
		$("i",this).addClass("open").parent().siblings().find("i").removeClass("open");
		$(".subnav").hide();
		$(this).parent().find("dl").show();
		return false;
	});
	
	$(".subnav a").live("click",function(){
		$(".faq_right_navul").empty();
		$(this).parent().siblings().find("a").removeClass("active");
		$(this).addClass("active").parent().parent().parent().siblings().find("a").removeClass("active");
		
		var id1=$(this).attr("data-id1");
		var id2=$(this).attr("data-id2");
		
		var data=info[id1].d[id2].d;
		var len = data.length;
		for (i=0;i<len;i++) {
			var liahtml='<li><a href="javascript:void(0)" data-id1="'+id1+'" data-id2="'+id2+'" data-id3="'+i+'" data-id1="" data-id='+id1+'-'+id2+'-'+i+' >'+(i+1)+'</a></li>';
			$(".faq_right_navul").append(liahtml);
		}
		$(".faq_right_navul li:last").addClass("last");
		
		//初始化
		$(".faq_right_navul a:first").click();	
		return false;	
	});
	
	$(".faq_right_navul a").live("click",function(){
		$(this).addClass("cur").parent().siblings().find("a").removeClass("cur")
		var id1=$(this).attr("data-id1");
		var id2=$(this).attr("data-id2");
		var id3=$(this).attr("data-id3");
		var data=info[id1].d[id2].d[id3];

		$(".faq_title").text(data.t);
		$(".faq_a_img img").attr("src",baseSrc + data.s);
		return false;
		
	});
	
	$(".faq_a_img").click(function(){
		var i=$(".faq_right_navul .cur").parent().index();
		i=i+1;
		if(i<$(".faq_right_navul a").length){$(".faq_right_navul a").eq(i).click();}	
	})
	$(".ul3 a:first").click();
	$(".ul3 a").eq(1).click();
	
	/*setTimeout(function(){
		$(".ul3 a:first").click();
		$(".ul3 a").eq(1).click();
		return false;
	},50)
	*/

	
})
</script>