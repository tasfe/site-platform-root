				if(pgeditor.pwdHash() == 0) { $("#login_pw_div").find(".error").text("请输入登录密码！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				if(pgeditor.pwdStrength() <= 2) { $("#login_pw_div").find(".error").text("对不起， 您输入的密码不符合规定，请输入7~23位数字、字母或特殊符号相结合的登录密码，空格除外！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				if(pgeditor1.pwdHash() == 0) { $("#re_login_pw_div").find(".error").text("请输入登录密码！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				if(pgeditor.pwdHash() != pgeditor1.pwdHash()) { $("#re_login_pw_div").find(".error").text("您输入的两次登录密码不一致，请重新输入！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				
				if(pgeditor2.pwdHash() == 0) { $("#pay_pw_div").find(".error").text("请输入支付密码！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				if(pgeditor2.pwdStrength() <= 2) { $("#pay_pw_div").find(".error").text("对不起， 您输入的密码不符合规定，请输入7~23位数字、字母或特殊符号相结合的支付密码，空格除外！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				if(pgeditor3.pwdHash() == 0) { $("#pay_pw2_div").find(".error").text("请输入支付密码！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				if(pgeditor2.pwdHash() != pgeditor3.pwdHash()) { $("#pay_pw2_div").find(".error").text("您输入的两次支付密码不一致，请重新输入！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				
				if(pgeditor.pwdHash() == pgeditor2.pwdHash()) { $("#pay_pw_div").find(".error").text("支付密码不能与登录密码相同，请重新输入！"); btn_submit.attr("disabled", false).removeClass("btn_next_disabled"); return false; }
				
