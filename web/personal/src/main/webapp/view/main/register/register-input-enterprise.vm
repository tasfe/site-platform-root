																	setPwdResult(pgeditor2,$('#pay_pw'),function(){
																		setPwdResult(pgeditor3,$('#pay_pw2'),function(){
																			_submit();
																		});
																	});
																});
															});
														});
													});
												});
											});
										});
									});
								});
							});
						});
					});
				});
			}
